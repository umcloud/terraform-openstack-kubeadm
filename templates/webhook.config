{
    "kind": "Config",
    "apiVersion": "v1",
    "preferences": {},
    "clusters": [
        {
            "name": "github-authn",
            "cluster": {
                "server": "http://127.0.0.1:3000/authenticate"
            }
        }
    ],
    "users": [
        {
            "name": "authn-apiserver",
            "user": {
                "token": "${random_token}"
            }
        }
    ],
    "contexts": [
        {
            "name": "webhook",
            "context": {
                "cluster": "github-authn",
                "user": "authn-apiserver"
            }
        }
    ],
    "current-context": "webhook"
}
